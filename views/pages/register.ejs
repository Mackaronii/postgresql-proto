<!DOCTYPE html>
<html>
  <head>
    <%- include ("../partials/header.ejs") %>
  </head>

  <body>
    <%- include ("../partials/nav.ejs") %>

    <div class="container">
		<h1>Register</h1>
		<form action="/register" method="POST">
		   <div>
			   <label for="username">Username</label>
			   <input type="username" id="username" name="username" required>
